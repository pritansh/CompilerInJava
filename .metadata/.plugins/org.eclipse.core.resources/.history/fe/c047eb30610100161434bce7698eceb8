package pc.compiler.symbolTable;

public class SymbolTableNode {

	private String varName,type;
	private String val;
	private SymbolTabel
	
	public SymbolTableNode() {
		varName = null;
		type = null;
		val = null;
	}
	
	public void add(String var, String type) {
		this.varName = var;
		this.type = type;
	}
	
}
